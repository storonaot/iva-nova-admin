{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "name": "member",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "full_name": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "maxLength": 100
    },
    "date_of_birth": {
      "type": "date",
      "required": true
    },
    "specialization": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "is_ex": {
      "type": "boolean",
      "default": false
    },
    "photos": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "main_photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "specialization_eng": {
      "type": "string",
      "required": false
    },
    "full_name_eng": {
      "type": "string",
      "required": false
    },
    "description_eng": {
      "type": "richtext",
      "required": false
    },
    "active_periods": {
      "type": "string"
    },
    "place_of_birth": {
      "type": "string"
    },
    "place_of_birth_eng": {
      "type": "string"
    }
  }
}
