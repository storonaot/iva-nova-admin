{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "name": "contact",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "maxLength": 100
    },
    "name_eng": {
      "type": "string",
      "minLength": 3,
      "maxLength": 100,
      "required": false
    },
    "position": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "maxLength": 100
    },
    "position_eng": {
      "type": "string",
      "required": false,
      "maxLength": 100,
      "minLength": 3
    },
    "phone_number": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    }
  }
}
